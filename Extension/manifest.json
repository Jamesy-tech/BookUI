{
  "manifest_version": 3,
  "name": "BookUI+ V1.0",
  "version": "1.0",
  "description": "Excute useful scripts into websites.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
